{
  "name": "pandacss-react-aria-components",
  "version": "1.0.3",
  "description": "A Panda preset for React Aria Components",
  "bugs": "https://github.com/kianomg/pandacss-react-aria-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/kianomg/pandacss-react-aria-components.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "NOTICE.txt"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "vitest run",
    "prepare": "npm run build",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "pandacss",
    "preset",
    "react-aria",
    "react-aria-components"
  ],
  "author": "kianomg",
  "license": "0BSD",
  "dependencies": {
    "@pandacss/types": "^0.47.0"
  },
  "devDependencies": {
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  }
}
